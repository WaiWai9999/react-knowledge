{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@lingui/react": ["node_modules/@lingui/react"],
      "@lingui/core": ["node_modules/@lingui/core"]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["./custom.d.ts"]
  },
  "include": [
    "src"
  ]
}
